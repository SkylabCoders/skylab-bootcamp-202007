	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 90,
				"lines": 80,
				"statements": -10
			}
		},
		"collectCoverageFrom": [
			"**/*.{js,jsx}",
			"!**/node_modules/**",
			"!**/vendor/**",
			"!**/coverage/**",
			"!**/components/**",
			"!**/*_CONFIG.js",
			"!**/*_TYPES.js",
			"!**/*_mock.js",
			"!**/*.json",
			"!**/*.spec.js",
			"!**/*.spec.js.snap"
		]
	}
}
