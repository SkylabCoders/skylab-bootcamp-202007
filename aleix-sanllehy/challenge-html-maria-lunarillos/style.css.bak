.divider {
  /*background-color: yellow;*/
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.divider-element {
  margin: 20px auto;
}

.divider-icon {
  width: 100px;
}

.divider-text {
  padding: 20px;
  border-top: 2px solid black;
  border-bottom: 2px solid black;
}

.main-content {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.main-content-box {
  display: flex;
}

.main-content-container {
  display: flex;
  flex-direction: column;
}

.box {
  /*border: 1px solid black;*/
  margin: 5px;
  width: 300px;
  height: 125px;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}

.box1-bg {
  background: url(https://static3.marialunarillos.com/media/wysiwyg/moldesdereposteria.jpg);
  background-size: cover;
}

.box2-bg {
  background: url(https://static3.marialunarillos.com/media/wysiwyg/utensiliosbasicos.jpg);
   background-size: cover;
}

.box3-bg {
  background: url(https://static3.marialunarillos.com/media/wysiwyg/pequenoelectrodomestico2.jpg);
  background-size: cover;
}

.box4-bg {
  background: url(https://static3.marialunarillos.com/media/wysiwyg/capsulas-cupcakes.jpg);
}

.box5-bg {
  background: url(https://static3.marialunarillos.com/media/wysiwyg/cajas3.jpg);
}

.box6-bg {
  background: url(https://static3.marialunarillos.com/media/wysiwyg/cortadoresgalletas2.jpg);
}

.box-innerdata {
  height: 60%;
  width: 50%;
  background-color: rgba(255, 255, 255, 0.8);
  display: flex;
  flex-direction: row;
  align-items: center;
}

.box-text {
  margin: 20px;
}

.box-details {
  margin-top: 10px;
  display: none;
}

.blog {
  margin: auto;
  width: 80%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.blog-button {
  background-color: rgb(255, 87, 89);
  margin: auto;
  margin-bottom: 10px;
  margin-top: 10px;
  padding: 10px;
  text-align: center;
}

.blog-last {
  width: 100%;
}

.blog-container {
  display: flex;
}

.blog-container .blog-card:nth-child(2) {
  display: none;
}

.blog-container .blog-card:nth-child(3) {
  display: none;
}

.blog-card {
  width: 200px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.blog-picture-img {
  width: 100%;
}

.blog-author {
  display: flex;
  margin-right: auto;
}

.blog-avatar-img {
  width: 50px;
}

.blog-text {
  margin-left: 10px;
  display: flex;
  flex-direction: column;
}

.prefooter {
  width: 100%;
  display: flex;
}

.prefooter-column {
  display: flex;
  flex-direction: column;
  width: 50%;
}

.prefooter-element {
  background-color: rgb(232, 230, 229);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
  .prefooter-element:hover{
    color: rgb(255, 87, 89);
  }

.prefooter-freeshipping {
  background: url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") 0 -38px no-repeat transparent;
  width: 85px;
  height: 60px;
}
  .prefooter-freeshipping:hover{
    background-position: 0 -109px;
  }

.prefooter-fastdelivery {
  background: url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") -90px -38px no-repeat transparent;
  width: 55px;
  height: 60px;
}
  .prefooter-fastdelivery:hover{
    background-position: -90px -109px;;
  }
.prefooter-return{
  width: 68px;
  background: url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") -150px -36px no-repeat transparent;
  height: 60px;
}
  .prefooter-return:hover{
    background-position: -150px -107px;
  }

.prefooter-payment {
  width: 77px;
  background: url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") -223px -38px no-repeat transparent;
  height: 60px;
}
  .prefooter-payment:hover{
    background-position: -223px -109px;
  }


