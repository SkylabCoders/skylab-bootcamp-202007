* {
  box-sizing: border-box;
  margin: 0;
  padding: 0;
}

body {
  font: 12px/1.55 "Source Sans Pro", sans-serif;
}

.header {
  top: 0;
  position: fixed;
  width: 100%;
  background-color: rgb(248, 248, 248);
  display: flex;
  height: 50px;
  margin: auto;
  justify-content: space-around;
  align-items: center;
}

.header-nav-left {
  display: flex;
}

.header-login {
  background: url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") 0 0 no-repeat;
  height: 27px;
  width: 27px;
}

.header-login:hover {
  background-position: -37px 0;
}

.header-search {
  background: #BBB8B6 url(https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg) -304px 12px no-repeat;
  height: 50px;
  width: 50px;
}

.header-cart {
  background: #fff url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") -66px 10px no-repeat;
  height: 50px;
  width: 50px;
}

.header-cart:hover {
  background: #FF5959 url("https://www.marialunarillos.com/skin/frontend/default/newdesign2017/images/sprite.svg") -105px 10px no-repeat;
}

.header-cart-count {
  background-color: rgb(255, 87, 89);
  color: rgb(255, 255, 255);
  height: 100%;
  width: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.subheader {
  padding: 50px 0 0 0;
  text-align: center;
  margin-bottom: 50px;
}

.slideshow {
  text-align: center;
}

.slideshow-img {
  height: 100%;
  width: 100%;
  max-width: 1024px;
}

