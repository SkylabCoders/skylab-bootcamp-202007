.foot {
  background-color: rgb(255, 87, 89);
  color: rgb(255, 255, 255);
  height: 50px;
  display: flex;
  align-items: center;
  justify-content: center;
}

@media only screen and (min-width: 600px) {
  .main-content {
      flex-direction: row;
  }

  .main-content-container {
    flex-direction: row;
  }

  .box {
    width: 150px;
    height: 300px;
  }

  .main-content-container .box:nth-child(1) {
      justify-content: flex-start;
  }

  .box-innerdata {
    height: 50%;
    width: 100%;
    justify-content: center;
  }

  .box-details {
    display: block;
  }

  .blog-container .blog-card:nth-child(2) {
    display: flex;
  }

}

@media only screen and (min-width: 1024px) {
  .main-content {
    flex-direction: row;
  }

  .main-content-container {
    flex-direction: column;
  }

  .box {
    height: 610px;
    width: 300px;
    flex-direction: row;
    align-items: flex-end;
    justify-content: flex-end;
  }

  .main-content-container .box {
    width: 600px;
    height: 300px;
  }

  .main-content-container .box:nth-child(1) {
    justify-content: flex-end;
  }

  .main-content-container:nth-child(2) .box:nth-child(1) {
    justify-content: flex-end;
  }

  .main-content-container .box:nth-child(2) {
    justify-content: flex-start;
  }

  .box-innerdata {
    height: 25%;
    width: 75%;
    display: flex;
    justify-content: center;
  }

  .main-content-container .box-innerdata {
    height: 50%;
    width: 40%;
  }

  .box-triangle {
    width: 0;
    height: 0;
    border-bottom: 150px solid rgba(255, 255, 255, 0.8);
    border-right: 100px solid transparent;
  }

  .blog-container .blog-card:nth-child(3) {
    display: flex;
  }

  /* .prefooter-column {
    flex-direction: row;
    justify-content: center;
  } */

}
